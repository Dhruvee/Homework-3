int count1=0;
int count2=2;
for(i=40;i<460;i=i+20){
  count1=count1 +1;
  if(count1%2==0){
    stroke(274,95,69);}
    else{
      stroke(296,49,78);}
  strokeWeight(5);
  line(i,40,40,i);}
  
   for(j=460;j>=40;j=j-20){
     count2=count2+1;
      if(count2%2==0){
    stroke(274,95,69);}
    else{
      stroke(296,49,78);}
     strokeWeight(5);
     line(460,j,j,460);}
     fill(0);
     noStroke();
     ellipse(250,250,200,200);
