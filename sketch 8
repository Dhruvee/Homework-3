background(0);
size(500,500);
colorMode(HSB,360,100,100);
float i,j;
int count1=0;
int count2=2;
for(i=40;i<460;i=i+20){
  count1=count1 +1;
  if(count1%2==0){
    stroke(174,92,85);}
    else{
      stroke(62,92,85);}
  strokeWeight(5);
  line(i,40,40,i);}
  
   for(j=460;j>=40;j=j-20){
     count2=count2+1;
      if(count2%2==0){
    stroke(174,92,85);}
    else{
      stroke(62,92,85);}
     strokeWeight(5);
     line(460,j,j,460);}
  
  
  
